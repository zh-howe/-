.shop-nav-text{
  width:228rpx;
  padding:20rpx 8rpx;
  display: flex;
  flex-direction: column;
}
.shop-nav-text text{
  font-size: 14px;
  color:#444;
  font-weight: bold;
}
.shop-nav-text text:nth-child(2){
  padding-top:8rpx;
  font-size: 12px;
  color:#888;
  font-weight: 500;
}
.discount-scroll{
	/* padding:10rpx 20rpx; */
	padding:10rpx 0;
	background:#f5f5f5;
}
.scroll-view{
	display: flex;
	flex-wrap: nowrap;
	padding:0 20rpx;
}
.discount-shop{
	min-width:200rpx;
	display: flex;
	flex-direction: column;
	background:#fff;
	margin-right:20rpx;
	/* margin-right:0; */
	border-radius: 5px;
	overflow: hidden;
}
.discount-text{
	padding:10rpx;
	display: flex;
	flex-direction: column;
}
.discount-shop-name{
	font-size: 12px;
	color:#555;
	display: inline-block;
	width: 100%;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.price{
	font-size: 15px;
	color:#BDA46A;
}
.discount-price{
	text-decoration:line-through;
	font-size: 12px;
	color:#bbb;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
	display: none;
}
.discountVip-shop{
