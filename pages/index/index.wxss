/**index.wxss**/

/* 引入 */
@import "../template/template.wxss";
/* 引入end */

/*模板css*/
.shop-title{
  display: flex;
  justify-content: space-between;
  padding:20rpx 30rpx;
  border-bottom:solid 1px #f5f5f5;
}
.shop-title text{
	color:#333;
	font-size: 13px;
	font-weight: bold;
}
.shop-title view{
	color:#888;
	font-size: 12px;
}
/*模板css end*/






.banner-swiper{
  position: relative;
}
.banner-swiper,.banner-swiper swiper,.banner-swiper image{
  width:750rpx;
  height:375rpx;
}
.banner-index{
  position: absolute;
  bottom:15px;
  right:15px;
  width:30px;
  height:30px;
  line-height: 28px;
  text-align: center;
  background:rgba(0,0,0,.5);
  border-radius: 50%;
}
.banner-index text{
  color:#fff;
  font-size: 12px;
  font-weight: 600;
}
.day-new{
  display: flex;
  height:100rpx;
  width:710rpx;
  padding:0 20rpx;
}
.day-new image{
  width:70rpx;
  height:70rpx;
  margin:15rpx 15rpx 0 0;
}
.day-new .scroll-news{
  width:625rpx;
  height:40rpx;
  line-height: 40rpx;
  margin:30rpx 0;
  overflow: hidden;
}
.day-new .scroll-news swiper{
  height:40rpx;
}
.day-new .scroll-news text{
  font-size: 12px;
  color:#888888;
}
.shop-nav{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.shop-nav-item{
  width:350rpx;
  height:120rpx;
  border:solid 1px #ddd;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}
.shop-nav-item navigator,.shop-nav-item{
	display: flex;
}
